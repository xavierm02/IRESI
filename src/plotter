set term png
set autoscale
unset log
unset label
set xtic auto
set ytic auto
set border 1+2 back

set yrange [0:1]

set xlabel "k"
set ylabel "Number of correct guesses / k"

set output "output/graph-1.png"
plot "output/graph-1.data" notitle with line linewidth 5

set output "output/graph-2.png"
plot "output/graph-2.data" notitle with line linewidth 5

set output "output/graph-3.png"
plot "output/graph-3.data" notitle with line linewidth 5
